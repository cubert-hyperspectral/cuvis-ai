# gRPC Server Configuration
GRPC_PORT=50051
GRPC_MAX_WORKERS=10

# TLS/SSL Configuration (optional)
# Set GRPC_USE_TLS=true to enable TLS
GRPC_USE_TLS=false
GRPC_TLS_CERT_PATH=/path/to/cert.pem
GRPC_TLS_KEY_PATH=/path/to/key.pem

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Data Configuration
# Default data directory for cu3s files
DATA_DIR=./data
